{
  "name": "@vivocha/extension-sdk",
  "version": "3.0.0-beta-esm.0",
  "description": "Vivocha Extensions and Channels SDK for Node.js",
  "type": "module",
  "main": "dist/index.js",
  "author": "Vivocha Development Team <dev@vivocha.com> (https://www.vivocha.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vivocha/extension-sdk.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "npm run build",
    "clean": "[ ! -d ./dist ] || rm -rf dist/*",
    "clean:all": "npm run clean && rm -rf node_modules",
    "build": "tsc",
    "build:all": "npm run clean:all && npm i && npm run build",
    "test": "find ./test/ts -type f -name '*.js' -delete && npm run build:tests && mocha --exit --no-timeouts --recursive --reporter mochawesome --reporter-options reportDir=test-results test/**/*.test.js",
    "test:debug": "npm run build && tsc -p test && mocha --inspect-brk --recursive test/**/*.test.js",
    "cover": "c8 --reporter=lcov --reporter=text npm t",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@vivocha/public-entities": "beta",
    "arrest": "^13.2.0",
    "aws-sdk": "^2.1168.0",
    "camelcase": "^7.0.0",
    "debuggo": "^1.4.1",
    "eredita": "^1.2.1",
    "express": "^4.18.1",
    "mongodb": "^4.7.0",
    "multer": "^1.4.4",
    "mustache": "^4.2.0",
    "needle": "^3.1.0",
    "request-promise-native": "^1.0.7",
    "serverless-http": "2.7.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "7.1.5",
    "@types/chai-spies": "1.0.3",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/mongodb": "^4.0.6",
    "@types/mustache": "^4.1.2",
    "@types/needle": "^2.5.3",
    "@types/node": "^16.11.26",
    "@types/request": "^2.48.8",
    "@types/request-promise-native": "^1.0.18",
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.1.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.2",
    "prettier": "^2.7.1",
    "request": "^2.88.2",
    "semantic-release": "^19.0.2",
    "supertest": "^6.2.4",
    "typescript": "^4.7.4"
  },
  "c8": {
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 0,
    "include": [
      "dist/**/*.js",
      "src/**/*.ts"
    ],
    "extension": [
      ".js",
      ".ts"
    ],
    "exclude": [
      "**/test"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true
  }
}
