{
  "name": "@vivocha/channel-dummy-channel",
  "version": "1.3.0-iengine.49",
  "description": "Vivocha Channel: Dummy Channel",
  "scripts": {
    "postpublish": "npm run build:aws",
    "clean": "[ ! -d ./dist ] || rm -rf dist/*",
    "build": "tsc",
    "build:all": "npm run build && npm run docker:build",
    "build:aws": "npm run build && npm run docker:build-aws && npm run docker:push-aws",
    "docker:build": "vvc-docker build -t ${VIVOCHA_RELEASE_TYPE:-release}",
    "docker:build-aws": "vvc-docker build -t ${VIVOCHA_RELEASE_TYPE:-release} --aws",
    "docker:push-aws": "vvc-docker push",
    "test": "find ./test/ts -type f -name '*.js' -delete && tsc -p test && ./node_modules/.bin/mocha --exit --no-timeouts --recursive --reporter-options reportDir=test-results test/**/*.test.js"
  },
  "files": [
    "README.md"
  ],
  "dependencies": {
    "@vivocha/extension-sdk": "^1.3.0-iengine.46",
    "arrest": "^9.1.0",
    "express": "^4.17.1",
    "memory-cache": "^0.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/memory-cache": "^0.2.0",
    "@types/mocha": "^7.0.1",
    "@vivocha/docker-utils": "^0.0.2-conversation.1",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "typescript": "^3.5.2"
  }
}
